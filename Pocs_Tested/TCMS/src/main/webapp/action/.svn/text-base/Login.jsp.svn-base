<%@ page contentType="text/html; charset=UTF-8" %>
<%@ taglib prefix="s" uri="/struts-tags" %>


<html>
<head>
    <title>TCMS Application</title>
</head>

<body>
<div id="main-container" class="outerDiv">

	<div class="headerDiv">		
		<%@ include file="header.jsp"%>		
	</div>

	<div id="sub-container" class="bodyDiv">
		<div id="login_form" class="centerDiv">	
				<table cellpadding="0" cellspacing="0" border="0" id="tbl-container" class="tbl-login-1">
				<tr>
				<td>	
					<s:form action="Login">
						<table cellpadding="0" cellspacing="0" border="0">
						<tr>
							<s:textfield key="username"/>
						</tr>
						<tr>
							<s:password key="password" />
						</tr>
						<tr class="row-btn">
						<td>&nbsp;</td>
						<td>
							<s:submit value="Login" />
						</td>
						</tr>
					</s:form>
				</td>
				</tr>
				</table>
		</div>
	</div>

</div>


</body>
</html>
